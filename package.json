{
  "name": "emoji-api",
  "version": "0.0.1",
  "description": "add, search and use emojis wherever you please with this RESTful API",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "echo 'CREATE DATABASE emoji_api;' | psql",
    "postinstall": "tsc && npm run migrate",
    "ts:compile": "tsc",
    "ts:lint": "tslint ./src/**/*.ts",
    "test": "mocha dist/test && npm run lint",
    "lint": "eslint --fix ./**/*.js",
    "start": "nodemon dist/index.js",
    "migrate": "knex migrate:latest --knexfile ./dist/db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile ./dist/db/knexfile.js"
  },
  "author": "TsubasaK111",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "knex": "^0.15.2",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "nodemon": "^1.18.3",
    "pg": "^6.4.0",
    "pg-native": "^1.10.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/knex": "^0.14.18",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.4",
    "awesome-typescript-loader": "^5.2.0",
    "chai": "^4.0.2",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-problems": "^0.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "expect": "^1.20.2",
    "mocha": "^3.4.2",
    "prettier": "^1.12.1",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
